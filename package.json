{
  "name": "reactorplate",
  "version": "1.0.0",
  "description": "A very opinionated boilerplate for React w/TypeScript",
  "main": "index.tsx",
  "author": "Shawn Harvell <sharvell@jbecker.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --open",
    "build": "webpack",
    "lint": "tsc --noEmit && eslint ./src",
    "lint:fix": "tsc --noEmit && eslint --fix ./src",
    "lint:sass": "sass-lint -v -q",
    "test": "jest --watch --coverage",
    "test:all": "jest --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "setuphusky": "node ./node_modules/husky/lib/installer/bin.js install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint ./src --fix-dry-run"
    ]
  },
  "devDependencies": {
    "@axe-core/react": "^4.1.1",
    "@babel/core": "^7.12.17",
    "@storybook/addon-a11y": "^6.1.18",
    "@storybook/addon-actions": "^6.1.18",
    "@storybook/addon-essentials": "^6.1.18",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/react": "^6.1.18",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-router-redux": "^5.0.18",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.2",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.0.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^28.0.2",
    "html-webpack-plugin": "^5.2.0",
    "husky": "4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.8",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "redux-mock-store": "^1.5.4",
    "sass-lint": "^1.13.1",
    "sass-loader": "^10.1.1",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "react-use": "^17.1.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  }
}
